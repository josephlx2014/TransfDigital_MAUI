<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TransfDigital_MAUI.MainPage">

    <StackLayout Spacing="1" x:Name="stack" Orientation="Horizontal" Margin="0,50,0,0">

        <BoxView Color="White" HeightRequest="40" WidthRequest="40"></BoxView>
        <BoxView Color="Blue" HeightRequest="40" WidthRequest="40"></BoxView>
        <BoxView Color="Yellow" HeightRequest="40" WidthRequest="40"></BoxView>

    </StackLayout>

</ContentPage>
